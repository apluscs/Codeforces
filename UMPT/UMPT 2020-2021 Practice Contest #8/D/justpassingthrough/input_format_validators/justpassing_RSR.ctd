# Three integers r c n, 3 <= r,c <= 500, 0 <= n <= 10
INT(3,500,r) SPACE INT(3,500,c) SPACE INT(0,10,n) NEWLINE

# r rows of c integers, all between -1 and 1000

SET(east=0)
SET(west=0)
REP(r)
  REPI(j,c,SPACE)
    INT(-1,1000,x)
    IF(j==0 && x >= 0)
      SET(west=1)
    END
    IF(j==c-1 && x >= 0)
      SET(east=1)
    END
  END
  NEWLINE
END
ASSERT(east==1)
ASSERT(west==1)
